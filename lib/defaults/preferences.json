{
  "app": {
    "info": "",
    "version": "0.0.0"
  },
  "enviroment": "dev",
  "enviromentMapping": {
    "enviroment": "APP_ENV"
  },
  "logging": {
    "fileLogging": true,
    "directory": "./tmp/log/",
    "info": "{{enviroment}}-info.log",
    "error": "{{enviroment}}-error.log"
  },
  "server": {
    "http": {
      "host": "localhost",
      "port": 3080
    },
    "https": false
  },
  "parse": {
    "appId": "parse",
    "appName": "parse",
    "masterKey": "parse",
    "server": {
      "mountPath": "/api",
      "database": "mongodb://localhost/parse"
    },
    "dashboard": {
      "mountPath": "/admin"
    },
    "admin": {
      "user": "admin",
      "pass": "admin"
    }
  }
}